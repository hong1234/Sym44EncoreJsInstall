{% extends 'base.html.twig' %}

{% block title %}Add Location{% endblock %}

{% block body %}
    <div id = "root" class="container">
        <img src="{{ asset('images/favicon.ico') }}" alt="missing image"/>
        {% for item in main_menu.getItems() %}
            <a href="{{ path(item.path) }}">{{ item.label }}</a> |
        {% endfor %}
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12">
                <br/>
                <h2 class="d-block p-3 bg-secondary text-white">Add Location Page</h2>
                {{ form(form) }}
            </div>
        </div>    
    </div>
{% endblock %}



